{
  "name": "Open RnD actions",
  "description": "Saves selection into Open R&D directory",
  "version": "1.0",
  "permissions": [ "contextMenus", "http://127.0.0.1:9000/"],
  "content_scripts" : [{
     "matches": ["http://*/*", "https://*/*"],
     "js" : ["openrnd.js"]
  }],
  "background": {
    "scripts": ["back.js"]
  },
  "commands" : {
      "select-parent": {
          "suggested_key":{
            "default": "Alt+Shift+Up"
          },
          "description" : "Select DOM parent"
      },
      "send-openrnd": {
          "suggested_key":{
            "default": "Alt+Shift+O"
          },
          "description" : "Send selection to Open RnD"
      }
  },
  "manifest_version": 2
}
